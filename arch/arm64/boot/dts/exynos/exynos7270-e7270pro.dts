	/* ST HUB */
	pinctrl@139F0000 {
		stm_hub_int_ap: stm-hub-int-ap {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		stm_ap_int_hub: stm-ap-int-hub {
			samsung,pins = "gpa0-4";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
			samsung,pin-pud = <0>;
		};
	};
	pinctrl@139C0000 {
		stm_boot0: stm-boot0 {
			samsung,pins = "gpc2-3";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
			samsung,pin-pud = <0>;
		};
	};

	pinctrl@139B0000 {
		stm_boot1: stm-boot1 {
			samsung,pins = "gpd1-4";
			samsung,pin-function = <1>;
			samsung,pin-val = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
		stm_rst_n: stm-rst-n {
			samsung,pins = "gpd1-2";
			samsung,pin-function = <1>;
			samsung,pin-val = <1>;
			samsung,pin-pud = <0>;
