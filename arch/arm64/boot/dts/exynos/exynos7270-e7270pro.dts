/dts-v1/;
#include "exynos7270.dtsi"

/ {
	model = "Exynos 7270 SMDK board";
	compatible = "samsung,exynos7570", "samsung,exynos4210";

	memory@40000000 {
		device_type = "memory";
		reg = <0x0 0x40000000 0x30000000>;
	};

	chosen {
		bootargs ="console=ttySAC2,115200 clk_ignore_unused androidboot.hardware=nerka androidboot.selinux=permissive androidboot.serialno=XXX firmware_class.path=/vendor/firmware ess_setup=0x46000000";
		linux,initrd-start = <0x42000000>;
		linux,initrd-end = <0x420FFFFF>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos7570-oscclk";
			clock-frequency = <26000000>;
		};
	};

	serial_2: uart@13820000 {
		status = "okay";
	};

        usi_1: usi@101D3000 {
                status = "okay";
        };

        serial_4: uart@13930000 {
                status = "okay";
        };

	dwmmc0@13540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		fixed_voltage;
		supports-highspeed;
		mmc-ddr-1_8v;
		supports-8bit;
		supports-cmd23;
		supports-erase;
		supports-hs400-enhanced-strobe;
		enable-ulp-mode;
		use-fine-tuning;
		only_once_tune;
		bypass-for-allpass;
		card-init-hwacg-ctrl;
		fifo-depth = <0x40>;
		non-removable;
		desc-size = <4>;
		card-detect-delay = <200>;
		clock-frequency = <700000000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		samsung,dw-mshc-hs400-delay-line = <0x60>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-hs400-timing = <1 0 2 0>;
		samsung,dw-mshc-hs400-ulp-timing = <3 0 2 0>;

		num-ref-clks = <12>;
		ciu_clkin = <25 43 43 25 43 87 175 43 43 175 175 175>;

		/* Swapping clock drive strength */
		clk-drive-number = <4>;
		pinctrl-names = "default",
			"fast-slew-rate-1x",
			"fast-slew-rate-2x",
			"fast-slew-rate-3x",
			"fast-slew-rate-4x";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		pinctrl-1 = <&sd0_clk_fast_slew_rate_1x>;
		pinctrl-2 = <&sd0_clk_fast_slew_rate_2x>;
		pinctrl-3 = <&sd0_clk_fast_slew_rate_3x>;
		pinctrl-4 = <&sd0_clk_fast_slew_rate_4x>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc2@13560000 {
		status = "disabled";
		num-slots = <1>;
		clock-gate;
//		fixed_volt;
		supports-highspeed;
		only_once_tune;
		enable-cclk-on-suspend;
		keep-power-in-suspend;
//		enable-sdio-wakeup;
                non-removable;
                broken-card-detect;
		supports-poweroff-notification;
		card-detect-ext;
//		cd-gpio = <&gpc0 1 0xf>;
		qos_int_level = <133>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;

		num-ref-clks = <11>;
		ciu_clkin = <25 50 50 50 50 100 200 100 100 200 400>;
//		clk-drive-number = <4>;
		pinctrl-names = "default";
//			"fast-slew-rate-1x",
//			"fast-slew-rate-2x",
//			"fast-slew-rate-3x",
//			"fast-slew-rate-4x";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4>;
//		pinctrl-1 = <&sd2_clk_fast_slew_rate_1x>;
//		pinctrl-2 = <&sd2_clk_fast_slew_rate_2x>;
//		pinctrl-3 = <&sd2_clk_fast_slew_rate_3x>;
//		pinctrl-4 = <&sd2_clk_fast_slew_rate_4x>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
//			disable-wp;
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";
	};

	usb@13600000 {
		status = "okay";

		dwc3 {
			dr_mode = "otg";
		};
	};

	usbphy@135c0000 {
		status = "okay";
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
		udc = <&udc>;
	};

	pinctrl@139F0000 {
		if_irq: if-irq {
			samsung,pins = "gpa0-5";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
	};

	i2c@13880000 {
		status = "okay";
		muic-s2mm001@25 {
			compatible = "samsung,muic-s2mm001";
			reg = <0x25>;
			pinctrl-names = "default";
			muic,muic_int = <&gpa1 1 0>;
		};
	};

	speedy@10510000 {
		status = "okay";
		s2mpw01mfd@00 {
			compatible = "samsung,s2mpw01mfd";
			reg = <0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&if_irq>;
			s2mpw01,irq-gpio = <&gpa0 5 0>;
			s2mpw01,wakeup = "enabled";

			i2c-speedy-address;
			cache_data = <1>;

			/* RTC: wtsr/smpl */
			wtsr_en = "enabled"; /* enable */
			wtsr_timer_val = <3>;   /* 1000ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: reset time to 1/1/2014 00:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <0>;
			init_time,mday = <1>;
			init_time,mon = <0>;    /* +1 */
			init_time,year = <116>; /* 1900 + 114 */
			init_time,wday = <5>;

			s2mpw01-keys {
				button@1 {
					label = "pmic-keys: KEY_POWER";
					linux,code = <116>;
					wakeup = <1>;
				};
			};

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "BUCK1";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1300000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
					regulator-expected-consumer = <6>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "BUCK3";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1500000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				buck4_reg: BUCK4 {
					regulator-name = "BUCK4";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <2100000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-initial-mode = <3>;
					regulator-ramp-delay = <12000>;
				};

				ldo1_reg: LDO1 {
					regulator-name = "vdd_ldo1";
					regulator-min-microvolt = <500000>;
					regulator-max-microvolt = <1350000>;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
					regulator-always-on;
					regulator-boot-on;
				};

				ldo2_reg: LDO2 {
