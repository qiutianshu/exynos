			&audio_bt_dummy
			&eax
			&eax
			&eax
			&eax>;
		samsung,audio-codec = <&audio_codec_dummy
			&audio_codec_dummy
			&audio_codec_dummy
			&audio_codec_dummy
			&audio_codec_dummy
			&audio_codec_dummy
			&audio_codec_dummy>;

		samsung,auxdev = <&s1403x>;
		status = "okay";
	};

	decon_fb@14830000 {
		/* HW TE */
		gpios = <&gpe0 2 0xf>;
		/* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		psr_mode = <2>;
		/* vclk_mif = 100MHz */
		mif-vclk = <100000000>;
		/* vclk = 12.5Mhz */
		disp-vclk = <12500000>;
		/* aclk = 208Mhz */
		disp-dvfs = <208000000>;
		status = "okay";
	};

	/* Display */
	pinctrl@139D0000 {
		p_rst: p_rst {
			samsung,pins = "gpd2-0";
			samsung,pin-con-pdn = <1>;
		};
	};

	dsim_0: dsim@14800000 {
		lcd_info = <&rm69080>;
		/* lcd reset */
		gpios = <&gpd2 0 0x1>;
		regulator_ldo16 ="vdd_ldo16";
		regulator_ldo17 ="vdd_ldo17";
		pinctrl-names = "default";
		pinctrl-0 = <&p_rst>;
		/* number of data lanes in use */
		data_lane_cnt = <1>;

		status = "okay";
	};

	rm69080: rm69080_custom {
		/* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		mode = <2>;
		/* custom (360 x 360) */
		panel_name = "rm69080";
		resolution = <400 400>;
		/* DISP I/F resolution width : 300, height : 432 */
		dispif_resol = <320 500>;
		size = <35 35>;
		timing,refresh = <60>;
		/* dsim H porch : hbp, hfp, hsa */
		timing,dsim_h-porch = <1 1 1>;
		/* dsim V porch : VBP, VFP, VSA */
		timing,dsim_v-porch = <10 2 1>;
		/* decon H porch : hbp, hfp, hsa */
		timing,decon_h-porch = <7 1 1>;
		/* decon V porch : VBP, VFP, VSA */
		timing,decon_v-porch = <10 2 1>;
		/* panel max speed : 384Mbps */
		timing,dsi-hs-clk = <384>;
		timing,pms = <2 118 2>;
		timing,dsi-escape-clk = <16>;
	};

	/* add for Broadcom bcm43438 */
	bcmdhd_wlan {
